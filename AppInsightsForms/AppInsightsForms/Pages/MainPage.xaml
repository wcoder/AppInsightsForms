<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
	xmlns="http://xamarin.com/schemas/2014/forms"
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	x:Class="AppInsightsForms.Pages.MainPage">
	
	<StackLayout>
		<Button Text="Click" Clicked="ClickClicked" />

		<Label Text="App:" />
		<Button Text="HandledException" Clicked="HandledExceptionClicked" />
		<Button Text="UnhandledException" Clicked="UnhandledExceptionClicked" />

		<Label Text="Native:" />
		<Button Text="ManagedException" Clicked="ManagedExceptionClicked" />
		<Button Text="UnmanagedException" Clicked="UnmanagedExceptionClicked" />

		<Button Text="Go to details" Clicked="GoDetailsClicked" />
	</StackLayout>

</ContentPage>